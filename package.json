{
  "name": "example-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "clean": "pnpm trash node_modules",
    "commit": "git-cz",
    "update-pnpm": "npm install -g pnpm@$(cat .pnpm-version | tr -d '\n\r')"
  },
  "engines": {
    "node": "v22.13.0",
    "pnpm": "9.15.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "packageManager": "pnpm@9.15.3",
  "private": true,
  "devDependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@nx/js": "20.7.1",
    "@nx/react": "20.7.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "nx": "20.7.1",
    "prettier": "^2.6.2",
    "trash-cli": "^6.0.0",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
